input=B
10

output :L
input:z
1
output:a


#include<stdio.h>
int main()
{
    int x;
    char c;
    scanf("%c",&c);

    scanf("%d",&x);
    if(c+x>'z')
    {
        c='a';
        printf("%c",c+x-1);
    }
    else if(c+x>'Z')
    {
        c='A';
        printf("%c",c+x-1);
    }
    else
    {
        printf("%c",c+x);
    }
}
