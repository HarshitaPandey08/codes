#include<stdio.h>
int main()
{
    int n,m,i,k,f=0;
    scanf("%d",&n);
    m=n*n;
    k=n;
    while(n>0)
    {
        if(n%10!=m%10)
        {
            f++;
            break;
        }
        n=n/10;
        m=m/10;
    }
    
    if(f==0)
    {
        printf("c");
    }
    else
    {
        printf("No");
    }
}
