#include<stdio.h>
#include<string.h>

int main()
{
    int t;
    scanf("%d",&t);
    while(t--)
    {
        char s[1000000];
        scanf("%s",s);
        int l,i,m,f=0;
        l=strlen(s);
        for(i=0;i<l;i++)
        {
            if(s[i]=='1')
            {
                m=i;
                f++;
            }
        }
        if(f!=0)
        {
            printf("%d\n",m);

        }
        else
        {
            printf("-1\n");
        }
    
    
    }
    
    
}
