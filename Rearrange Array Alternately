#include<stdio.h>
int main()
{
	int t,m,n,i;
	scanf("%d",&t);
	while(t>0)
	{
		scanf("%d",&m);
		int a[m],b[n],c[m+n];
		for(i=0;i<m;i++)
		{
			scanf("%d",&a[i]);
		}
		
		int i=0,j=0,k=0;
		for(i=0,j=m-1;i<m/2;i++,j--)
		{
			if(j>=m/2)
			{
				printf("%d %d ",a[j],a[i]);
			}
		}
		if(m%2!=0)
		{
			printf("%d",a[m/2]);
		}
		printf("\n");
		t--;
	
		
	}
}
