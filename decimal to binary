#include<stdio.h>
int main()
{
    int a[100],i=0,f=1,t,n;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d",&n);
        i=0;
        while(n>0)
        {
            a[i]=n%2;
            i++;
            n=n/2;
        }
        for(int k=i-1;k>=0;k--)
        {
            printf("%d ",a[k]);
        }
        printf("\n");
    }
    
}
