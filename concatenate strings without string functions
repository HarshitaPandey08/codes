#include<string.h>
#include<stdio.h>
int main()
{
    char s[100],t[100];
    scanf("%s",s);
    scanf("%s",t);
    int l1,l2;
    l1=strlen(s);
    l2=strlen(t);
    char m[l1+l2];
    int i,j;
    for(i=0;i<l1;i++)
    {
        m[i]=s[i];
        
    }
    for(j=0;j<l2;j++)
    {
        m[i]=t[j];
        i++;
    }
    m[i]='\0';
    printf("%s",m);

    

}
