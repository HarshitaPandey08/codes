input=6 2
1 2 3 4 5 6
output
5 6 1 2 3 4


#include<stdio.h>
int main()
{
    int n,i=0,j,t,k=0;
    scanf("%d%d",&n,&t);
    int a[n],b[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        
    }
    j=n;
    for(i=0;i<n;i++)
    {
        b[k++]=a[(n-t+i)%n];
    }
    for(i=0;i<j;i++)
    {
        printf("%d ",b[i]);
    }
    
}
