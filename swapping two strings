#include<stdio.h>
#include<string.h>
int main()
{
    char a[10],b[10];
    int i,n;
    scanf("%s",a);
    scanf("%s",b);
    
        n=strlen(a)>strlen(b)?strlen(a):strlen(b);
        for(i=0;i<n;i++)
        {
        char t=a[i];
        a[i]=b[i];
        b[i]=t;
        }

    
    a[i]='\0';
    b[i]='\0';
    for(i=0;i<n;i++)
    {
        printf("%c",a[i]);
    }
    for(i=0;i<n;i++)
    {
        printf("%c",b[i]);
    }
}
