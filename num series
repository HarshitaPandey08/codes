//1,1,3,2,9,4,27,8,81,16.....
#include<stdio.h>

int main()
{
    int n,a=1,b=1,i;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(i%2==0)
        {
            printf("%d,",a);
            a=a*2;
        }
        else
        {
            printf("%d,",b);
            b=b*3;
        }
    }
}
