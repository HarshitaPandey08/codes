#include<stdio.h>
int main()
{
    char s[100];
    scanf("%[^\n]s",s);
    int a[26]={0};
    int i,v=0;
    for(i=0;s[i]!='\0';i++)
    {
        a[s[i]-97]++;
        
    }
    for(i=0;i<26;i++)
    {
        if(a[i]==0)
        {
            v++;
            break;
        }
    }
    if(v==0)
    {
        printf("pangram");
    }
    else
    {
    	printf("no");
	}
}
